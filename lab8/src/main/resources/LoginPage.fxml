<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane prefHeight="350.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.gui.Controller">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0" style="-fx-padding: 0; -fx-border-insets: 5px;" BorderPane.alignment="CENTER">
               <children>
                  <Text fx:id="login_login_text" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <TextField fx:id="login_login_field" />
                  <Text fx:id="login_password_text" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <PasswordField fx:id="login_password_field" />
                  <Button fx:id="login_login_button" mnemonicParsing="false" text="Button" />
                  <Button fx:id="login_register_button" mnemonicParsing="false" text="Button" />
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </center>
         <left>
            <VBox prefHeight="200.0" prefWidth="50.0" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <VBox prefHeight="200.0" prefWidth="50.0" BorderPane.alignment="CENTER" />
         </right>
         <bottom>
            <VBox prefHeight="100.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <VBox prefHeight="50.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Text fx:id="login_info_text" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
                     <font>
                        <Font size="19.0" />
                     </font></Text>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </top>
      </BorderPane>
   </children>
</StackPane>
